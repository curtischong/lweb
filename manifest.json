{
  "manifest_version": 2,
  "name": "lweb",
  "version": "0.1",
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["dependencies/jquery-3.3.1.min.js", "dependencies/moment-with-locals.js", "typer.js"]
  }],
  "browser_action": {
    "default_icon": "logo.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}